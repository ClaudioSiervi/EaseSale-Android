<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.siervi.claudio.easysale">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".EasySale">
        <activity android:name="com.siervi.claudio.easysale.MainActivity"> <!-- Parent Activity -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.siervi.claudio.easysale.SalesActivity" />
        <activity android:name="com.siervi.claudio.easysale.ProductRegistrationActivity" />
        <activity android:name="com.siervi.claudio.easysale.ReportActivity"
            android:parentActivityName="com.siervi.claudio.easysale.MainActivity"/>

    </application>

</manifest>
